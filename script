cat test.sol | grep 'h(' | grep -v ') 0' > rusl.txt
sed -i 's/,/ /g' rusl.txt
sed -i 's/h(/ /g' rusl.txt
sed -i 's/)/ /g' rusl.txt
echo "param hfix := " >  hfix.dat
cat rusl.txt >> hfix.dat
echo ";" >> hfix.dat
echo "end;" >> hfix.dat
glpsol --math BessyRooms4.mod -d courses.dat -d default.dat  -d resources.dat -d RoomData.dat -d SplitForPhase.dat -d RoomsAndBuildings.dat -d conjoined.dat -d hfix.dat
